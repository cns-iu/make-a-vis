    "ngx-markdown": "^11.1.3",
    "node-fetch": "^2.6.1",
    "papaparse": "^5.3.0",
    "pbf": "^3.2.1",
    "pdfkit-browserify": "^0.8.3-R2",
    "rxjs": "^6.6.7",
    "save-svg-as-png": "^1.4.17",
    "svg-to-pdfkit": "^0.1.8",
    "topojson-client": "^3.1.0",
    "tslib": "^2.2.0",
    "typescript-logging": "^1.0.0",
    "vega": "^5.20.2",
    "vega-embed": "^6.17.0",
    "vega-lite": "^5.1.0",
    "zipcodes": "^8.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1102.10",
