    "@angular/router": "^11.2.11",
    "@angular/service-worker": "^11.2.11",
    "@ngrx/effects": "^11.1.1",
    "@ngrx/store": "^11.1.1",
    "@ngx-dino/core": "^0.11.0",
    "@ngx-dino/legend": "^0.13.0",
    "@turf/boolean-point-in-polygon": "^6.3.0",
    "@turf/centroid": "^6.3.0",
    "@turf/helpers": "^6.3.0",
    "blob-stream-browserify": "^0.1.3",
    "cheet.js": "^0.3.3",
    "core-js": "^3.11.0",
    "css-element-queries": "^1.2.3",
    "d3-geo-projection": "^3.0.0",
    "d3-scale": "^3.3.0",
    "d3-selection": "^2.0.0",
    "d3-transition": "^2.0.0",
    "file-saver": "^2.0.5",
